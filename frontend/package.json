{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Simple Frontend for CO2 server",
  "main": "src/index.html",
  "repository": "https://github.com/joshkunz/co2",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/chance": "^1.1.1",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.11.2",
    "@types/react": "^17.0.0",
    "@types/react-bootstrap": "^0.32.25",
    "@types/react-dom": "^17.0.0",
    "babel-jest": "^26.6.3",
    "chance": "^1.1.7",
    "gts": "^3.1.0",
    "jest": "^26.6.3",
    "msw": "^0.26.1",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.3"
  },
  "browserslist": [
    "> 10%",
    "not dead"
  ],
  "jest": {
    "globals": {
      "ts-jest": {
        "useESM": true
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist"
    ]
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
